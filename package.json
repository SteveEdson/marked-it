{
  "name": "marked2-marked-it",
  "version": "1.0.2",
  "description": "Custom processor for Marked2, using marked-it with plugins",
  "main": "index.js",
  "scripts": {
    "test": "ava tests/",
    "test-update": "ava tests/ -u",
    "postinstall": "markedpath=$(which markedit); echo \"Use '$markedpath' as the custom processor path under advanced settings\"",
    "semantic-release": "semantic-release --prepare && npm publish && semantic-release --verify-release"
  },
  "bin": {
    "markedit": "./index.js",
    "marked-it": "./index.js"
  },
  "keywords": [
    "marked",
    "marked2",
    "markdown-it",
    "markdown",
    "md",
    "processor",
    "custom"
  ],
  "author": "Steve Edson <steve@steveedson.co.uk>",
  "license": "MIT",
  "repository": "https://github.com/SteveEdson/marked-it",
  "dependencies": {
    "get-stdin": "^6.0.0",
    "highlight.js": "^9.12.0",
    "markdown-it": "^8.4.2",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emoji": "^1.4.0"
  },
  "devDependencies": {
    "ava": "1.0.0-beta.7",
    "semantic-release": "^15.9.8"
  }
}
